CC		=	gcc

DEBUG		=	true

WALL		=	true

ifeq	($(WALL),true)
 CFLAGS		+=	-W -Wall -Werror
endif

ifeq	($(DEBUG),true)
 CFLAGS		+=	-g3
endif
